// to use this custom model you need to set the following option in the config.yml
// graph.elevation.provider: srtm   # enables elevation
// graph.encoded_values: foot_access, foot_priority, foot_average_speed, foot_road_access, hike_rating, mtb_rating, average_slope, country, road_class
// profiles:
//    - name: foot
//      custom_model_files: [foot.json, foot_elevation.json]

{
  "priority": [
    { "if": "!foot_access", "multiply_by": "0" },
    { "else": "", "multiply_by": "foot_priority"},
    { "if": "country == DEU && road_class == BRIDLEWAY", "multiply_by": "0" }
  ],
  "speed": [
    { "if": "true", "limit_to": "foot_average_speed" },
    { "if": "surface == MISSING", "limit_to": "1" },
    { "if": "surface == PAVED", "limit_to": "7" },
    { "if": "surface == ASPHALT", "limit_to": "7" },
    { "if": "surface == CONCRETE", "limit_to": "6" },
    { "if": "surface == PAVING_STONES", "limit_to": "5" },
    { "if": "surface == COBBLESTONE", "limit_to": "3" },
    { "if": "surface == UNPAVED", "limit_to": "2.5" },
    { "if": "surface == COMPACTED", "limit_to": "3" },
    { "if": "surface == FINE_GRAVEL", "limit_to": "2.8" },
    { "if": "surface == GRAVEL", "limit_to": "2.5" },
    { "if": "surface == GROUND", "limit_to": "2.5" },
    { "if": "surface == DIRT", "limit_to": "2.5" },
    { "if": "surface == GRASS", "limit_to": "2" },
    { "if": "surface == SAND", "limit_to": "1" },
    { "if": "surface == WOOD", "limit_to": "4" },
    { "if": "surface == OTHER", "limit_to": "3" },
    {"if": "road_class == OTHER", "limit_to": "4" },
    { "if": "road_class == MOTORWAY", "limit_to": "0" },
    { "if": "road_class == TRUNK", "limit_to": "0" },
    { "if": "road_class == PRIMARY", "limit_to": "0" },
    { "if": "road_class == SECONDARY", "limit_to": "2" },
    { "if": "road_class == TERTIARY", "limit_to": "4" },
    { "if": "road_class == RESIDENTIAL", "limit_to": "6" },
    { "if": "road_class == UNCLASSIFIED", "limit_to": "4" },
    { "if": "road_class == SERVICE", "limit_to": "6" },
    { "if": "road_class == ROAD", "limit_to": "4" },
    { "if": "road_class == TRACK", "limit_to": "2" },
    { "if": "road_class == BRIDLEWAY", "limit_to": "0" },
    { "if": "road_class == STEPS", "limit_to": "0" },
    { "if": "road_class == CYCLEWAY", "limit_to": "0" },
    { "if": "road_class == PATH", "limit_to": "5" },
    { "if": "road_class == LIVING_STREET", "limit_to": "7" },
    { "if": "road_class == FOOTWAY", "limit_to": "7" },
    { "if": "road_class == PEDESTRIAN", "limit_to": "7" },
    { "if": "road_class == PLATFORM", "limit_to": "5" },
    { "if": "road_class == CORRIDOR", "limit_to": "4" },
    { "if": "road_class == CONSTRUCTION", "limit_to": "0" },
    { "if": "mtb_rating > 2", "limit_to": "0" },
    { "if": "hike_rating > 1", "limit_to": "0"},
    { "if": "!foot_subnetwork", "multiply_by": "0.9"},
    { "if": "foot_network == INTERNATIONAL", "multiply_by": "1.1"},
    { "if": "foot_network == NATIONAL", "multiply_by": "1.1"},
    { "if": "foot_network == REGIONAL", "multiply_by": "1.1"},
    { "if": "road_environment == OTHER", "multiply_by": "1" },
    { "if": "road_environment == ROAD", "multiply_by": "1" },
    { "if": "road_environment == FERRY", "multiply_by": "0.5" },
    { "if": "road_environment == TUNNEL", "multiply_by": "0.3" },
    { "if": "road_environment == BRIDGE", "multiply_by": "0.8" },
    { "if": "road_environment == FORD", "multiply_by": "0" }
  ]
}